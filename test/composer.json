{
  "repositories": [
    {
      "type": "path",
      "url": "../",
      "options": {
          "versions": {
              "files.com/files-php-sdk": "dev-local"
          }
      },
      "extra": {
          "branch-alias": {
              "dev-local": "0.0.1"
          }
      },
      "symlink": false 
    }
  ],
  "require": {
    "files.com/files-php-sdk": "dev-local",
    "php": ">=5.5.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6 || ^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^8.0"
  }
}